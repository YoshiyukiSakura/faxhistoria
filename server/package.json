{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run --exclude='**/e2e/**'",
    "test:e2e": "playwright test --config playwright.config.ts",
    "test:all": "vitest run --exclude='**/e2e/**' && playwright test --config playwright.config.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@faxhistoria/shared": "*",
    "@prisma/client": "^6.2.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^17.3.1",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.1.0",
    "openai": "^4.77.0",
    "uuid": "^11.0.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.10.5",
    "@types/uuid": "^10.0.0",
    "prisma": "^6.2.1",
    "tsx": "^4.19.2",
    "typescript": "^5.5.0",
    "vitest": "^2.1.8"
  }
}
